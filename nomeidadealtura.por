programa {
  funcao inicio() {
    escreva("Qual seu nome:\n")
    cadeia nome
    leia(nome)
    escreva("Qual sua idade:\n")
    inteiro idade
    leia(idade)
    escreva("Qual sua altura?\n")
    real altura
    leia(altura)
    escreva("Nome:\n", nome)
    escreva("\nIdade:\n", idade)
    escreva("\nAltura:\n", altura)
  }
}
